{
    "name": "<%= _.slugify(projectName) %>",
    "description": "Prototype",
    "version": "0.1.0",
    "author": {
        "name": "<%= authorLogin %>",
        "email": "<%= authorEmail %>",
        "url": "http://github.com/<%= _.slugify(authorLogin) %>/"
    },
    "engines": {
        "node": ">=0.8"
    },
    "scripts": {
        "test": "grunt test"
    },
    "devDependencies": {
		"load-grunt-tasks": "~0.3.0",
		"grunt": "~0.4.2", <% if(installAssemble === true){ %>
        "assemble": "~0.4.34", <% } %><% if(installDocs === true){ %>
        "grunt-styleguide": "~0.2.14", 
        "grunt-contrib-copy": "~0.5.0", <% } %>
        "grunt-newer": "~0.5.4",<% if(modules && modules.length > 0){ %>
		<% if(typeof modules === 'object'){ _.each(modules, function(name, i) { 
		if(name == 'grunt-grunticon') { %>
		"grunt-grunticon": "~1.0.0-beta.0",
		<% } %><% if(name == 'dr-grunt-svg-sprites') { %>
		"dr-grunt-svg-sprites": "~0.2.6",
        <% } %><% if(name == 'grunt-contrib-compass') { %>
        "grunt-contrib-compass": "*",
		<% } %><% if(name == 'grunt-devtools') { %>
		"grunt-devtools": "~0.2.1",
		<% } %><% if(name == 'grunt-packager') { %>
		"grunt-packager": "~0.1.0",
		<% } %><% if(name == 'grunt-browser-sync') { %>
		"grunt-browser-sync": "~0.3.0",
		<% } %><% if(name == 'grunt-htmlhint') { %>
		"grunt-htmlhint": "~0.4.0",
		<% } %><% if(name == 'grunt-prettysass') { %>
		"grunt-prettysass": "~0.2.3",
		<% } %><% if(name == 'grunt-combine-media-queries') { %>
		"grunt-combine-media-queries": "~1.0.9",
		"grunt-contrib-cssmin": "~0.7.0",
		<% } %><% if(name == 'grunt-bless') { %>
		"grunt-bless": "~0.1.1",
		<% } %><% if(name == 'grunt-photobox') { %>
		"grunt-photobox": "~0.5.0",
		<% } %><%}); %><%} %><%} %>
		"grunt-sync": "~0.0.5",
		"grunt-concurrent": "~0.4.2",
        "grunt-contrib-clean": "~0.5.0",
		"grunt-bg-shell": "*",
        "grunt-contrib-connect": "~0.5.0",
        "grunt-contrib-watch": "~0.5.3",
        "load-grunt-tasks": "~0.1.3",
        "grunt-timer": "~0.1.0"<% if(plugin && plugin.length > 0){ %>,
<% if(typeof plugin === 'object'){ _.each(plugin, function(name, i) { %>
    "<%= name %>": "*"<% if(i < (plugin.length - 1)) { %>,<% } %>
<% }); } else { %>
    "<%= plugin %>": "*"
    <%} } %>
},
"licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/<%= _.slugify(authorLogin) %>/<%= _.slugify(projectName) %>/blob/master/LICENSE-MIT"
    }
  ]
}
