{
    "name": "<%= _.slugify(projectName) %>",
    "description": "Prototype",
    "version": "0.1.0",
    "author": {
        "name": "<%= authorLogin %>",
        "email": "<%= authorEmail %>",
        "url": "http://github.com/<%= _.slugify(authorLogin) %>/"
    },
    "engines": {
        "node": ">=0.8"
    },
    "scripts": {
        "test": "grunt test"
    },
    "devDependencies": {
		"grunt": "~0.4.5", <% if(installAssemble != false){ %>
        "assemble": "~0.4.42",
		"lodash" : "*",
		"gray-matter": "*",
		"handlebars-helper-autolink": "^0.1.1",<% } %><% if (features && features.length > 0) { if (features.indexOf('sassInsteadOfCompass') != -1) { %>
        "grunt-sass": "^0.16.0",<% }} %><% if ((features && features.length > 0 && features.indexOf('sassInsteadOfCompass') != -1) || (modules && modules.length > 0 && modules.indexOf('grunt-responsive-images') != -1)) { %>
        "grunt-fileindex": "^0.1.0",<% } %><% if (features && features.length > 0) { if (features.indexOf('mobileFirst') != -1) { %>
        "grunt-comment-media-queries": "~0.1.0", <% }} %><% if (features && features.length > 0) { if (features.indexOf('installDocs') != -1) { %>
        "grunt-styleguide": "~0.2.14",<% }} %><% if (features && features.length > 0 && (features.indexOf('installDocs') != -1 || features.indexOf('createDevFolder') != -1)) { %>
        "grunt-contrib-copy": "~0.7.0",<% } %>
        "grunt-newer": "~0.8.0",<% if(modules && modules.length > 0){ %>
		<% if(typeof modules === 'object'){ _.each(modules, function(name, i) { 
		if(name == 'grunt-grunticon') { %>
		"grunt-grunticon": "~1.0.0-beta.0",
		<% } %><% if(name == 'grunt-dr-svg-sprites') { %>
		"grunt-dr-svg-sprites": "*",
        <% } %><% if(name == 'grunt-dr-svg-sprites' || name == "grunt-grunticon") { %>
		"grunt-text-replace": "~0.3.11",
        <% } %><% if(name == 'grunt-contrib-compass') { %>
        "grunt-contrib-compass": "*",
		<% } %><% if(name == 'grunt-devtools') { %>
		"grunt-devtools": "~0.2.1",
		<% } %><% if(name == 'grunt-packager') { %>
		"grunt-packager": "~0.1.0",
		<% } %><% if(name == 'grunt-contrib-requirejs') { %>
		"grunt-contrib-requirejs": "~0.4.4",
		<% } %><% if(name == 'grunt-jsdoc') { %>
		"grunt-jsdoc": "^0.6.1",
		<% } %><% if(name == 'grunt-contrib-uglify') { %>
		"grunt-contrib-uglify": "~0.5.1",
		<% } %><% if(name == 'grunt-modernizr') { %>
		"grunt-modernizr": "^0.5.2",
		<% } %><% if(name == 'grunt-browser-sync') { %>
		"grunt-browser-sync": "~1.5.3",
		<% } %><% if(name == 'grunt-csscomb') { %>
		"grunt-csscomb": "^3.0.0",
		<% } %><% if(name == 'grunt-combine-media-queries') { %>
		"grunt-combine-media-queries": "~1.0.19",
		<% } %><% if(name == 'grunt-bless') { %>
		"grunt-bless": "~0.1.1",
		<% } %><% if(name == 'grunt-autoprefixer') { %>
		"grunt-autoprefixer": "~0.6.5",
		<% } %><% if(name == 'grunt-data-separator') { %>
		"grunt-data-separator": "~0.1.3",
		<% } %><% if(name == 'grunt-accessibility') { %>
		"grunt-accessibility": "~0.1.2",
        <% } %><% if(name == 'grunt-connect-proxy') { %>
        "grunt-connect-proxy": "~0.1.8",
		<% } %><% if(name == 'grunt-svgmin') { %>
        "grunt-svgmin": "^1.0.0",
		<% } %><% if(name == 'grunt-phantomas') { %>
        "grunt-phantomas": "^0.10.0",
		<% } %><% if(name == 'grunt-responsive-images') { %>
        "grunt-responsive-images": "^0.1.3",
		<% } %><% if(name == 'grunt-version') { %>
        "grunt-version": "^0.3.0",
		<% } %><% if(name == 'grunt-photobox') { %>
		"grunt-photobox": "~0.8.0",
		<% } %><%}); %><%} %><%} %>
		"jit-grunt": "^0.9.0",
		"load-grunt-configs": "~0.4.1",<% if (features.indexOf('sassInsteadOfCompass') == -1) { %>
        "grunt-bg-shell": "*",<% } %>
		"grunt-sync": "~0.2.0",
		"grunt-htmlhint": "~0.4.0",
		"grunt-contrib-htmlmin": "~0.3.0",
		"grunt-concurrent": "~0.4.2",
		"grunt-jsbeautifier": "~0.2.6",
		"grunt-contrib-cssmin": "~0.10.0",
        "grunt-contrib-clean": "~0.6.0",
        "grunt-contrib-connect": "~0.8.0",
        "grunt-contrib-watch": "~0.6.1",
		"grunt-contrib-jshint": "~0.8.0",
        "time-grunt": "*"<% if(plugin && plugin.length > 0){ %>,
<% if(typeof plugin === 'object'){ _.each(plugin, function(name, i) { %>
    "<%= name %>": "*"<% if(i < (plugin.length - 1)) { %>,<% } %>
<% }); } else { %>
    "<%= plugin %>": "*"
    <%} } %>
},
"licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/<%= _.slugify(authorLogin) %>/<%= _.slugify(projectName) %>/blob/master/LICENSE-MIT"
    }
  ] <% if(installAssemble != false){ %>,
  "keywords": [
		"handlebars-helper-autolink"
	]<% } %>
}
